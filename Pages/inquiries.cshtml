@page
@model PrivacyModel
@{
    ViewData["Title"] = "Inquiries";
}

<form method="post">
<h6>Inquiries:</h6>

<table class="table">
    <thead>
    <th>Request</th>
    <th>From</th>
    <th>Done</th>
    </thead>
    <tbody>
        @{
            for (int i = 0; i < @Model.inqtot; i++)
            {
                <tr>
                    <td>@Model.comment[i]</td>
                    <td>@Model.sender[i]</td>
                    <td>@Model.done[i]</td>
                </tr>
            }
        }
    </tbody>
</table>

<button class="btn btn-outline-info">Chane inquiry status</button>

</form>